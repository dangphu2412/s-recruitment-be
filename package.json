{
  "name": "backend-nestjs",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": ">=v22.14.0",
    "pnpm": ">=9.0.0"
  },
  "packageManager": "pnpm@9.12.2",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "upload": "pnpm run build && ts-node src/upload-fingerprint-trigger.ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "db:config": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli",
    "migration:create": "npm run db:config migration:create ./src/system/database/migrations/$npm_config_name",
    "migration:generate": "npm run db:config migration:generate ./src/system/database/migrations/$npm_config_name",
    "migration:run": "npm run db:config -- -d src/system/database/config/migration.config.ts migration:run",
    "migration:revert": "npm run db:config -- -d src/system/database/config/migration.config.ts migration:revert",
    "schema:drop": "npm run db:config -- -d src/system/database/config/migration.config.ts schema:drop",
    "schema:sync": "npm run db:config -- -d src/system/database/config/migration.config.ts schema:sync",
    "db:reload": "npm run migration schema:drop && npm run migrate:latest",
    "prepare": "husky install"
  },
  "dependencies": {
    "@fastify/compress": "^7.0.1",
    "@fastify/helmet": "11.1.1",
    "@fastify/swagger": "^8.15.0",
    "@google-cloud/storage": "^7.15.2",
    "@nestjs/cache-manager": "^2.3.0",
    "@nestjs/common": "10.4.13",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "10.4.13",
    "@nestjs/event-emitter": "^2.1.1",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-fastify": "^10.4.13",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/serve-static": "4.0.2",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/typeorm": "^10.0.2",
    "@react-email/components": "^0.1.1",
    "@react-email/render": "^1.1.3",
    "bcryptjs": "^2.4.3",
    "cache-manager": "^4.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "0.14.0",
    "csv-parser": "^3.1.0",
    "date-fns": "^2.30.0",
    "dotenv": "16.1.4",
    "etag": "^1.8.1",
    "fastify": "4.28.1",
    "fastify-multer": "^2.0.3",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.35",
    "ms": "^2.1.3",
    "nestjs-pino": "^4.4.0",
    "node-zklib": "^1.3.0",
    "nodemailer": "^7.0.4",
    "object-path": "^0.11.8",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pg": "8.11.3",
    "pino-http": "^10.5.0",
    "pino-pretty": "^13.1.1",
    "qs": "^6.11.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "reflect-metadata": "0.1.13",
    "resend": "^4.6.0",
    "rimraf": "^5.0.8",
    "rxjs": "7.2.0",
    "typeorm": "^0.3.17",
    "typeorm-transactional": "^0.5.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@commitlint/cli": "16.2.3",
    "@commitlint/config-angular": "16.2.3",
    "@nestjs/cli": "^10.4.9",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^10.4.13",
    "@swc/cli": "0.3.12",
    "@swc/core": "1.4.16",
    "@types/bcryptjs": "^2.4.2",
    "@types/cache-manager": "^4.0.1",
    "@types/etag": "^1.8.3",
    "@types/jest": "^28.1.3",
    "@types/lodash": "^4.14.198",
    "@types/mime-types": "^2.1.4",
    "@types/morgan": "^1.9.9",
    "@types/ms": "^0.7.31",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.0.0",
    "@types/nodemailer": "^6.4.17",
    "@types/passport-jwt": "^3.0.6",
    "@types/qs": "^6.9.16",
    "@types/react-dom": "^19.1.6",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^8.0.3",
    "jest": "^28.1.3",
    "lint-staged": "^15.2.7",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^28.0.8",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "3.10.1",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.18.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "modulePathIgnorePatterns": [
      "dist",
      ".module.ts",
      ".exception.ts",
      "index.ts",
      "main.ts"
    ],
    "coveragePathIgnorePatterns": [
      "node_modules/",
      ".module.ts",
      "client",
      "dtos",
      "index.ts",
      "main.ts",
      ".dto.ts",
      ".entity.ts",
      ".interface.ts",
      ".enum.ts",
      ".constant.ts",
      "/src/system/database/migrations",
      "/src/system/database/config"
    ],
    "moduleNameMapper": {
      "src/(.*)": "<rootDir>/src/$1"
    },
    "coverageThreshold": {
      "global": {
        "branches": 45,
        "functions": 60,
        "lines": 65
      }
    }
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint",
      "npm run format"
    ]
  }
}
