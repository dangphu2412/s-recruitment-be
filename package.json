{
  "name": "backend-nestjs",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migration": "ts-node -r tsconfig-paths/register node_modules/typeorm/cli.js --config src/system/database/config/migration.config.ts",
    "seeder": "ts-node -r tsconfig-paths/register node_modules/typeorm/cli.js --config src/system/database/config/seeder.config.ts",
    "migrate:create": "npm run migration migration:create -- -n",
    "migrate:generate": "npm run migration migration:generate -- -n",
    "migrate:latest": "npm run migration migration:run",
    "migrate:rollback": "npm run migration migration:revert",
    "seeder:create": "npm run seeder migration:create -- -n",
    "seeder:generate": "npm run seeder migration:generate -- -n",
    "seeder:latest": "npm run seeder migration:run",
    "seeder:rollback": "npm run seeder migration:revert",
    "schema:drop": "npm run migration schema:drop",
    "schema:sync": "npm run migration schema:sync",
    "db:reload": "npm run migration schema:drop && npm run seeder schema:drop && npm run migrate:latest && npm run seeder:latest",
    "prepare": "husky install"
  },
  "dependencies": {
    "@fastify/compress": "^6.4.0",
    "@fastify/helmet": "10.1.1",
    "@fastify/swagger": "^7.6.1",
    "@nestjs/cache-manager": "^2.1.0",
    "@nestjs/common": "9.4.3",
    "@nestjs/config": "^2.1.0",
    "@nestjs/core": "9.4.3",
    "@nestjs/jwt": "^10.0.0",
    "@nestjs/passport": "^10.0.1",
    "@nestjs/platform-fastify": "^9.4.3",
    "@nestjs/swagger": "^6.3.0",
    "@nestjs/typeorm": "^9.0.0",
    "bcryptjs": "^2.4.3",
    "cache-manager": "^4.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "0.14.0",
    "date-fns": "^2.29.3",
    "fastify-multer": "^2.0.3",
    "lodash.isempty": "^4.4.0",
    "lodash.map": "^4.6.0",
    "lodash.uniq": "^4.5.0",
    "lodash.uniqby": "^4.7.0",
    "lodash.xor": "^4.5.0",
    "multer": "^1.4.5-lts.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pg": "8.7.1",
    "qs": "^6.11.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.2.0",
    "slugify": "^1.6.5",
    "typeorm": "^0.3.17",
    "typeorm-transactional-cls-hooked": "^0.1.21",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@commitlint/cli": "16.2.3",
    "@commitlint/config-angular": "16.2.3",
    "@nestjs/cli": "^9.5.0",
    "@nestjs/schematics": "^9.2.0",
    "@nestjs/testing": "^9.4.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/cache-manager": "^4.0.1",
    "@types/date-fns": "^2.6.0",
    "@types/express": "^4.17.13",
    "@types/jest": "27.4.1",
    "@types/lodash.isempty": "^4.4.7",
    "@types/ms": "^0.7.31",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.0.0",
    "@types/passport-jwt": "^3.0.6",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.3",
    "jest": "^27.2.5",
    "prettier": "^2.3.2",
    "rimraf": "3.0.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "3.10.1",
    "typescript": "4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      ".module.ts",
      "client",
      "dtos"
    ]
  }
}
